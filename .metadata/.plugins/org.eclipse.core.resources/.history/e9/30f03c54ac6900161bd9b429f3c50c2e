package org.demo.dao;

import java.util.Scanner;

public class Test{
	static int counter=10;
	public static void main(String [] args)
    {
		int i;
		for(i=1;i<=10;i++);
		{
    			if(i<5)
				       System.out.print(" "+i/2);
			    else
       				System.out.print(" "+i%2);
		}
		System.out.print("  "+i);

    }

	}
